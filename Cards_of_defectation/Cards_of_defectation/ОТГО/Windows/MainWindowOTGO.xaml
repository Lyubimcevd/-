<Window x:Class="Cards_of_defectation.ОТГО.Windows.MainWindowOTGO"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cards_of_defectation.ОТГО.Windows"
        mc:Ignorable="d"
        Title="Ремонтные заказы" 
        Height="158" 
        Width="300" 
        ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Button Content="Создать служебную записку" 
                Click="Create_new_SZ"/>
        <Button Content="Открыть служебную записку" 
                Grid.Row="1" 
                Click="Open_SZ"/>
    </Grid>
</Window>
